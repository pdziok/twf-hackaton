{
    "name": "funteam/fungame",
    "description": "Fun Game",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
    ],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "silex/silex": "~1.2",
        "igorw/config-service-provider": "~1.2",
        "marcojanssen/silex-routing-service-provider": "~1.3",
        "doctrine/collections": "~1.3.0",
        "monolog/monolog": "~1.16",
        "symfony/twig-bridge": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "phpunit/phpcov": "~2.0",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~2.0",
        "pdepend/pdepend":"~2.0",
        "sebastian/phpcpd": "~2.0",
        "mayflower/php-codebrowser": "~1.1",
        "squizlabs/php_codesniffer": "~2.2",
        "fabpot/php-cs-fixer": "~1.0",
        "symfony/browser-kit": "~3.0@dev",
        "symfony/css-selector": "~3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "FunGame\\": "src/FunGame/",
            "ServiceProvider\\": "src/ServiceProvider/"
        }
    }
}
